@using System.Collections
@model RunningObjects.MVC.Member
@{
    var count = 0;
    if (Model.Value != null)
    {
        var coll = Model.Value as IEnumerable;
        if(coll != null)
        {
            count = Queryable.Count((dynamic) coll.AsQueryable());
        }
    }
    <text>@count</text>
}
